<table>
  <thead>
    <tr>
      
      <th data-field="new-url">New Url</th>
      <th data-field="clicks">Clicks</th>
      <!-- <th data-field="clicks">Details</th> -->
    </tr>
  </thead>
  <tbody>
  
    <% @popular_links.each do |f| %>
      <tr>
        <td><%= link_to "#{truncate "#{root_url}#{f.short_url}", length: 26 }", f.short_url, :target => "_blank" %></td>
        <td><%= f.clicks %></td>

      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @popular_links, param_name: :popular_links, :remote => true %>
